<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Playwright TestNG Suite" parallel="methods" thread-count="4">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="Parallel Sample Tests">
        <classes>
            <class name="steps.SampleTest"/>
        </classes>
    </test>
</suite>




